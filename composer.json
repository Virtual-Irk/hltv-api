{
  "name": "virtual-irk/hltv-api",
  "description": "PHP Hltv API Client",
  "keywords": [
    "csgo",
    "csgoapi",
    "api",
    "hltv",
    "hltv.org",
    "cs"
  ],
  "type": "library",
  "homepage": "https://github.com/virtual-irk/hltv-api",
  "license": "MIT",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Konstantin Repin",
      "email": "repin.and.co@gmail.com",
      "homepage": "https://github.com/webrepin"
    },
    {
      "name": "Aleksey Belyaev",
      "email": "38virtual@gmail.com",
      "homepage": "https://github.com/virtual-irk"
    }
  ],
  "require": {
    "php": ">=7.4",
    "ext-curl": "*",
    "guzzlehttp/guzzle": "^7.2",
    "sunra/php-simple-html-dom-parser": "1.5.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0",
    "codeclimate/php-test-reporter": "dev-master"
  },
  "suggest": {
    "guzzlehttp/guzzle": "Allows for implementation of the Guzzle HTTP client"
